<%- include('../templates/admin-header') %>

<div id="dashboard">
    <div class="d-flex">
        <%- include('../templates/admin-sidebar')%>
        <div class="content">
            <div class="container pb-5">
                <div class="page-title mt-5">
                    <h1>CONTACT PAGE</h1>
                </div>
                <div class="row mt-5">
                    <div class="col-12 col-md-12 mb-4">
                        <label class="mb-2">Facebook Link</label>
                        <input type="text" name="fb-link" id="fb-link" class="form-control" placeholder="Write Here...">
                    </div>
                    <div class="col-12 col-md-12 mb-4">
                        <label class="mb-2">Instagram Link</label>
                        <input type="text" name="ig-link" id="ig-link" class="form-control" placeholder="Write Here...">
                    </div>
                    <div class="col-12 col-md-12 mb-4">
                        <label class="mb-2">Email</label>
                        <input type="text" name="email" id="email" class="form-control" placeholder="Write Here...">
                    </div>
                    <div class="col-12 col-md-12 mb-4">
                        <label class="mb-2">Linked Link</label>
                        <input type="text" name="fb-link" id="fb-link" class="form-control" placeholder="Write Here...">
                    </div>
                    <div class="col-12 d-flex justify-content-end mt-5">
                        <button class="btn btn-primary p-2">Save Changes</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<%- include('../templates/admin-footer') %>
